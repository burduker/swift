= partial 'base/index_operations'
%form.inner.multiple#multiple-elements{ :action => url(:elements, :index)/:multiple, :method => :post }
  %table.table{ :class => @objects.length > 15 && 'smart' }
    %thead
      %tr
        %th.first
        %th= mat(:object, :id)
    %tbody
      - @objects.each do |object|
        %tr
          %td.last= mk_checkbox object
          %td.first= link_to object.id, url(:elements, :edit, :id => object.id)
= partial 'base/index_operations_bottom'
