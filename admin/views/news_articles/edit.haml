.block
  = partial 'base/edit_operations'
  .content
    %h2.title
      = pat(:edit)
      = mt(:news_article)
    .inner
      - form_for :object, url(:news_articles, :update, :id => @object.id), :method => :put, :class => 'form edit', :id => "edit-news_articles" do |f|
        = partial "news_articles/form", :locals => { :f => f }
