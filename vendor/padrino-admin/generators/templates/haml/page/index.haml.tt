= partial 'base/index_operations'
%form.inner.multiple#multiple-<%= @orm.name_plural %>{ :action => url(:<%= @orm.name_plural %>, :index)/:multiple, :method => :post }
  %table.table
    %thead
      %tr
        %th.first= mat(:object, :id)
        %th.last= check_box_tag :check_all, :checked => false
    %tbody
      - @objects.each do |object|
        %tr
          %td.first= link_to object.id, url(:<%= @orm.name_plural %>, :edit, :id => object.id)
          %td.last= mk_checkbox object
